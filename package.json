{
  "name": "availity-github-io",
  "description": "Homge Page for Availity's Open Source Libraries",
  "version": "0.1.0",
  "author": "Kyle Gray <kyle.gray@availity.com>",
  "dependencies": {
    "@availity/app-icon": "^1.2.0",
    "@availity/gatsby-theme-core": "^1.2.11",
    "gatsby": "^2.13.83",
    "gatsby-plugin-compile-es6-packages": "^2.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-icons": "^3.5.0",
    "reactstrap": "^7.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-preset-gatsby": "^0.1.10",
    "eslint-config-availity": "^4.0.2",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "deploy": "npm run build && gh-pages -d public -b master",
    "deploy:ci": "npm run deploy",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
